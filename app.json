{
  "expo": {
    "name": "AquaIntel",
    "slug": "aquaintel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF9933"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aquaintel.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FF9933"
      },
      "package": "com.aquaintel.app",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "PLACEHOLDER_WILL_BE_REPLACED_AT_BUILD_TIME"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "blockedPermissions": [
        "READ_PHONE_STATE",
        "READ_CONTACTS",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "READ_CALL_LOG",
        "WRITE_CALL_LOG",
        "READ_SMS",
        "RECEIVE_SMS",
        "SEND_SMS"
      ],
      "jsEngine": "hermes",
      "architectures": ["arm64-v8a"]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "minSdkVersion": 24,
            "buildToolsVersion": "35.0.0",
            "usesCleartextTraffic": true,
            "enableProguardInReleaseBuilds": true,
            "enableHermes": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.facebook.hermes.unicode.** { *; }\n-keep class com.facebook.jni.** { *; }\n-dontwarn com.facebook.react.**",
            "useLegacyPackaging": false,
            "networkInspector": false,
            "packagingOptions": {
              "pickFirst": [
                "lib/arm64-v8a/libc++_shared.so"
              ],
              "exclude": [
                "lib/x86/**",
                "lib/x86_64/**",
                "lib/armeabi-v7a/**",
                "lib/armeabi/**",
                "lib/mips/**",
                "lib/mips64/**"
              ]
            }
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c8380ef2-81a3-4816-9cf7-9c722ea15087"
      }
    }
  }
}
